<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.dao.AdminDao">
    <insert id="insert" parameterType="AdminDto">
        INSERT INTO admin_info (`admin_id`, `admin_name`, `admin_pwd`, `admin_phone_no`, `admin_email`, `reg_id`)
        VALUES(#{id}, #{name}, #{pwd}, #{phone_no}, #{email}, #{admin_id});
    </insert>

    <select id="count" resultType="int">
        SELECT COUNT(*) FROM admin_info
    </select>
</mapper>